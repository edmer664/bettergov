{
  "name": "gov-ph-portal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run generate:metadata && tsx ./scripts/write-ver-to-json.ts && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:report": "playwright show-report",
    "wrangler": "wrangler",
    "preview": "vite preview",
    "prepare": "husky",
    "functions:dev": "wrangler dev ./functions/index.ts",
    "functions:build": "tsc -p ./functions/tsconfig.json",
    "functions:deploy": "wrangler deploy ./functions/index.ts",
    "index:meilisearch": "node ./scripts/index_meilisearch.cjs",
    "index:meilisearch:reset": "node ./scripts/index_meilisearch.cjs --reset",
    "index:flood-control:arcgis": "node ./scripts/index_flood_control_arcgis.js",
    "index:flood-control:arcgis:reset": "node ./scripts/index_flood_control_arcgis.js --reset",
    "index:create-key": "node ./scripts/create_search_key.cjs",
    "index:contractor-profiles": "node ./scripts/index_contractors.cjs",
    "index:all": "npm run index:meilisearch && npm run index:flood-control:arcgis && npm run index:contractor-profiles",
    "process:unique-contractors": "node ./scripts/process_contractors.cjs",
    "fetch:contractor-profiles": "node ./scripts/fetch_contractor_profiles.cjs",
    "generate:llms-txt": "node ./scripts/generate-llms-txt.js",
    "generate:sitemap": "node ./scripts/generate-sitemap.js",
    "generate:metadata": "npm run generate:llms-txt && npm run generate:sitemap"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.7.0",
    "@meilisearch/instant-meilisearch": "^0.26.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/react-leaflet": "^3.0.0",
    "clsx": "^2.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "instantsearch.css": "^8.5.1",
    "jsdom": "^26.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.513.0",
    "meilisearch": "^0.50.0",
    "nuqs": "^2.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.3",
    "react-instantsearch": "^7.15.8",
    "react-leaflet": "5.0.0-rc.2",
    "react-router-dom": "^6.22.2",
    "react-world-flags": "^1.6.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^2.2.1",
    "wrangler": "^4.19.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/workers-types": "^4.20250607.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/glob": "^8.1.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "commander": "^13.1.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "glob": "^10.4.5",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "uuid": "^9.0.1",
    "vite": "^5.4.2"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
